# Node Settings
ROOT=/chainlink
CHAINLINK_TLS_PORT=0
LOG_LEVEL=debug
CHAINLINK_DEV=true

# Network Configuration
ETH_CHAIN_ID=84532
LINK_CONTRACT_ADDRESS=0xE4aB69C077896252FAFBD49EFD26B5D171A32410
MIN_OUTGOING_CONFIRMATIONS=1

# Required Database Settings (not in original command but required for node operation)
DATABASE_URL=postgresql://postgres:chainlink@postgres:5432/chainlink_dev?sslmode=disable

# Security Settings (recommended defaults)
SECURE_COOKIES=false
ALLOW_ORIGINS=*

# Feature Flags (recommended for v2)
FEATURE_LOG_POLLER=true
FEATURE_EVM_V2=true
FEATURE_FEEDS_MANAGER=true

# API
ETH_URL=wss://base-sepolia.g.alchemy.com/v2/<YOUR_API_KEY>
ETH_HTTP_URL=https://base-sepolia.g.alchemy.com/v2/<YOUR_API_KEY>